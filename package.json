{
  "name": "hyperapp-rollup-starter",
  "version": "1.0.0",
  "description": "A boilerplate project for Hyperapp with Rollup",
  "scripts": {
    "lint": "standard --fix src/*.js | snazzy",
    "build": "rollup -c rollup.config.js -w",
    "build:prod": "rollup -c rollup.config.prod.js",
    "serve": "npx serve"
  },
  "keywords": [
    "hyperapp",
    "rollup"
  ],
  "author": "zoron (https://github.com/fralonra/)",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.4.0",
    "babel-plugin-transform-react-jsx": "^6.24.1",
    "hyperapp": "^1.2.9",
    "rollup": "^1.7.2",
    "rollup-plugin-babel": "^4.3.2",
    "rollup-plugin-cpy": "^1.1.0",
    "rollup-plugin-delete": "^0.2.1",
    "rollup-plugin-filesize": "^6.0.1",
    "rollup-plugin-livereload": "^1.0.0",
    "rollup-plugin-node-resolve": "^4.0.1",
    "rollup-plugin-serve": "^1.0.1",
    "snazzy": "^8.0.0",
    "standard": "^12.0.1"
  }
}
